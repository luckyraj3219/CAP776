num_of_books = int(input("Enter the number of books: "))

def my_function():
    Title = input("Enter the title of book: ")
    Author = input("Enter the author of book: ")
    Price = input("Enter the price of book: ")
    Stock = input("Enter the stock quantity for a book: ")
    return {"Title": Title, "Author": Author, "Price": Price, "Stock": Stock}

books = []
for i in range(num_of_books):
    book_info = my_function()
    books.append(book_info)

print(books)
